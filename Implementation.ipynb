    "from sklearn.datasets import fetch_california_housing\n",
    "from sklearn.model_selection import KFold\n",
    "def load_data():\n",
    "    data= fetch_california_housing()\n",
    "    print(\"Features of Boston Housing Prices dataset: \", data.get(\"feature_names\"))\n",
    "    print(\"Shape of the X: \", data.data.shape)\n",
    "    print(\"Shape of the Y: \", data.target.shape)\n",
    "    return data.data, data.target\n",
    "\n",
    "\n",
    "\n",
    "def multiple_gradient_descent_KFold(X, y, learning_rate = 0.000001, max_its = 10000):\n",
    "    \n",
    "    # --- Normalize X ---\n",
    "    mu = np.mean(X, axis=0)\n",
    "    sigma = np.std(X, axis=0)\n",
    "    X_norm = (X - mu) / sigma\n",
    "    \n",
    "    # Add bias column of ones\n",
    "    ones = np.ones((X_norm.shape[0], 1))\n",
    "    X_norm = np.hstack([ones, X_norm])  # shape: (N, 9)\n",
    "    \n",
    "    # Prepare Kfold\n",
    "    kf = KFold(n_splits=5, shuffle=True, random_state=42)\n",
    "    \n",
    "    test_mse_list = []\n",
    "\n",
    "    fold_id = 1\n",
    "    for train_index, test_index in kf.split(X_norm):\n",
    "        \n",
    "        print(f\"\\n==================== Fold {fold_id} ====================\")\n",
    "        \n",
    "        X_train, X_test = X_norm[train_index], X_norm[test_index]\n",
    "        y_train, y_test = y[train_index], y[test_index]\n",
    "        \n",
    "        N_train = len(y_train)\n",
    "        \n",
    "        # Initialize betas\n",
    "        beta = np.zeros(X_train.shape[1])  # 9 parameters\n",
    "        \n",
    "        # Gradient descent\n",
    "        for k in range(max_its):\n",
    "            # predictions\n",
    "            y_hat = X_train @ beta\n",
    "            \n",
    "            # error\n",
    "            error = y_train - y_hat\n",
    "            \n",
    "            # gradient\n",
    "            grad = (-2.0 / N_train) * (X_train.T @ error)\n",
    "            \n",
    "            # update\n",
    "            beta = beta - learning_rate * grad\n",
    "            \n",
    "            # print training loss every 1000 steps\n",
    "            if (k+1) % 1000 == 0:\n",
    "                mse_train = np.mean(error**2)\n",
    "                print(f\"Iter {k+1}: train MSE = {mse_train:.4f}\")\n",
    "        \n",
    "        # Evaluate on test fold\n",
    "        y_pred_test = X_test @ beta\n",
    "        test_mse = np.mean((y_test - y_pred_test)**2)\n",
    "        \n",
    "        test_mse_list.append(test_mse)\n",
    "        print(f\"Fold {fold_id} Test MSE = {test_mse:.4f}\")\n",
    "        \n",
    "        fold_id += 1\n",
    "    \n",
    "    # After all folds\n",
    "    print(\"\\nFinal Test MSEs for all folds:\", test_mse_list)\n",
    "    print(\"Variance of Test MSEs:\", np.var(test_mse_list))\n",
    "    \n",
    "    return test_mse_list"
   ]
